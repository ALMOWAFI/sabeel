# Sabeel Platform Environment Configuration

# API Base URL
VITE_API_BASE_URL=http://localhost:5000

# Feature Flags
VITE_ENABLE_LOGGING=true
VITE_ENABLE_SUPABASE_AUTH=true
VITE_TENSORFLOW_CLIENT_SIDE=false

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-goes-here

# TensorFlow.js Configuration
VITE_TFJS_MODEL_PATH=/assets/models
VITE_TFJS_TRAINING_ENDPOINT=/api/tfjs/training
VITE_TFJS_INFERENCE_ENDPOINT=/api/tfjs/inference
VITE_TFJS_MODELS_ENDPOINT=/api/tfjs/models
VITE_TFJS_USE_GPU=true

# Integration API Endpoints
VITE_EDX_API_ENDPOINT=https://api.sabeel.app/edx
VITE_CANVAS_API_ENDPOINT=https://api.sabeel.app/canvas
VITE_PEERTUBE_API_ENDPOINT=https://api.sabeel.app/peertube
VITE_H5P_API_ENDPOINT=https://api.sabeel.app/h5p
VITE_JUPYTER_API_ENDPOINT=https://api.sabeel.app/jupyterbook
VITE_KINGRAPH_API_ENDPOINT=https://api.sabeel.app/kingraph
VITE_SUPABASE_API_ENDPOINT=https://api.sabeel.app/supabase
VITE_TENSORFLOW_API_ENDPOINT=https://api.sabeel.app/tfjs
